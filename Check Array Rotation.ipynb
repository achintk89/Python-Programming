    "def countRotations(arr, n): \n",
    "\n",
    "\tmin = arr[0] \n",
    "\tfor i in range(0, n): \n",
    "\t\n",
    "\t\tif (min > arr[i]): \n",
    "\t\t\n",
    "\t\t\tmin = arr[i] \n",
    "\t\t\tmin_index = i \n",
    "\t\t\n",
    "\treturn min_index; \n",
    "\n",
    "\n",
    "n = int(input()) \n",
    "arr = [int(x) for x in input().split()] \n",
    "print(countRotations(arr, n)) "
   ]
