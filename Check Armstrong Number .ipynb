   "source": [
    "n = int(input())\n",
    "\n",
    "sum = 0\n",
    "length=len(str(n))\n",
    "temp = n\n",
    "while temp > 0:\n",
    "   digit = temp % 10\n",
    "   sum += digit ** length\n",
    "   temp //= 10\n",
    "\n",
    "\n",
    "if n == sum:\n",
    "   print(\"true\")\n",
    "else:\n",
    "   print(\"false\")"
   ]
  }
