    "def move_zero(num_list):\n",
    "\n",
    "    a = [0 for i in range(num_list.count(0))]\n",
    "\n",
    "    x = [ i for i in num_list if i != 0]\n",
    "\n",
    "    x.extend(a)\n",
    "\n",
    "    return(x)\n",
    "\n",
    "\n",
    "n=int(input())\n",
    "arr=move_zero([int(x) for x in input().split()])\n",
    "for le in arr:\n",
    "    print(le,end=\" \")\n",
    "\n"
   ]
