    "N = int(input())\n",
    "\n",
    "\n",
    "reverse_N = 0\n",
    "\n",
    "\n",
    "while(N>0):\n",
    " \n",
    "  remainder = N % 10\n",
    "\n",
    "  \n",
    "  reverse_N = (reverse_N * 10) + remainder\n",
    "\n",
    "\n",
    "  N = N//10\n",
    "\n",
    "\n",
    "print(format(reverse_N))"
   ]
  }
