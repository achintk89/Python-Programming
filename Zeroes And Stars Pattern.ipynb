    "n = int(input())\n",
    "i = 1\n",
    "while i <= n:\n",
    "  j = 1\n",
    "  while j <= (2 * n) + 1:\n",
    "    if(j==i or j== n + 1 or (2*n + 2)-j==i):\n",
    "      print(\"*\",end = '')\n",
    "    else:\n",
    "      print(\"0\",end = '')\n",
    "    j = j + 1\n",
    "  print()\n",
    "  i = i + 1"
   ]
